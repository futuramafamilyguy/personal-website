{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbConfiguration": {
    "DatabaseName": "PersonalWebsite",
    "PicturesCollectionName": "pictures",
    "CinemasCollectionName": "cinemas"
  },
  "FileImageStorageConfiguration": {
    "PictureImageSubDirectory": "pictures",
    "AllowedImageExtensions": [
      ".jpg",
      ".jpeg",
      ".png",
      ".webp"
    ]
  }
}
